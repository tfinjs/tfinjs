  },
  "homepage": "https://tfinjs.com",
  "dependencies": {
    "@tfinjs/api": "file:../api",
    "@tfinjs/dependency-graph": "^0.0.2-beta",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "figures": "^2.0.0",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
