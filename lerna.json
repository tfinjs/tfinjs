{
  "version": "0.2.2",
  "npmClient": "yarn",
  "packages": [
    "api",
    "aws-lambda",
    "aws-lambda-packager",
    "cli",
    "demos/*"
  ],
  "command": {
    "publish": {
      "message": "chore(release): publish %s",
